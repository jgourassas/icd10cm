<%= form_for @changeset, @action, fn f -> %>
  <%= if @changeset.action do %>
    <div class="alert alert-danger">
      <p>Oops, something went wrong! Please check the errors below.</p>
    </div>
  <% end %>

  <%= label f, :productid %>
  <%= textarea f, :productid %>
  <%= error_tag f, :productid %>

  <%= label f, :productndc %>
  <%= textarea f, :productndc %>
  <%= error_tag f, :productndc %>

  <div>
    <%= submit "Save" %>
  </div>
<% end %>
